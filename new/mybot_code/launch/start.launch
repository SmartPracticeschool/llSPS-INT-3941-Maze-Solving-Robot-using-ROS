    <param name="initial_y" value="$(arg initial_y)" />
    <param name="des_pos_x" value="$(arg des_x)" />
    <param name="des_pos_y" value="$(arg des_y)" />
    <node pkg="mybot_code" type="wall.py" name="wall_follower" />
    <node pkg="mybot_code" type="point_motion.py" name="go_to_point" />
    <node pkg="mybot_code" type="mazesolver1.py" name="bug0" output="screen" />
