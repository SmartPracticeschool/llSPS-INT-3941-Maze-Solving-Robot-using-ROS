    <param name="des_pos_y" value="$(arg des_y)" />
    <node pkg="mybot_code" type="wall.py" name="wall_follower" output="screen" />
    <node pkg="mybot_code" type="point_motion.py" name="go_to_point" output="screen" />
    
</launch>
